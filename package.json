{
  "name": "guld-env-cli",
  "version": "1.0.13",
  "description": "Guld environment detection CLI.",
  "bin": {
    "guld-env": "cli.js"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint --env browser *.js*",
    "fix": "eslint --fix *.js*",
    "test": "mocha test/*.js",
    "test:lint": "npm run lint && npm run test"
  },
  "keywords": [
    "guld",
    "cli",
    "OS detection",
    "browser detection",
    "distro detection",
    "environment",
    "configuration",
    "detection",
    "node"
  ],
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.17.1",
    "guld-env": "^2.0.6"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "pre-commit": "^1.2.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "npm-check-updates": "^2.14.2"
  },
  "pre-commit": [
    "lint"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://guld.tech/cli/guld-env-cli.html",
  "repository": "https://bitbucket.org/guld/tech-js-node_modules-guld-env-cli"
